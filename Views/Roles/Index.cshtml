@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewBag.Title = "Roles";
}
<html>
<head>

</head>
<body>
    <div>
        <a href="@Url.Action("Create","Roles")" class="btn btn-primary">Create New</a>
        <table class="table table-hover table-bordered table-active">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>                       
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>